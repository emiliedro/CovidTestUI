<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.covidtestui">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ContactDoctorForm"></activity>
        <activity android:name=".UpcomingTestsPatient" />
        <activity android:name=".ContactTracingForm" />
        <activity android:name=".SymptomForm" />
        <activity android:name=".CovidTestForm" />
        <activity android:name=".AntibodyTestForm" />
        <activity android:name=".PatientMore" />
        <activity android:name=".SymptomLog" />
        <activity android:name=".TestResults" />
        <activity android:name=".BookTest" />
        <activity android:name=".PatientHome" />
        <activity android:name=".PatientLogin" />
        <activity android:name=".DoctorLogin" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>